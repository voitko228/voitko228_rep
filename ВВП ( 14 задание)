#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "RUS");
    //1
    {
        int a, b;
        cout << "Задание 1" << endl;
        cout << "Введите два целых положительных числа A и B (A < B): ";
        cin >> a >> b;

        for (int i = a; i <= b; i++) {
            for (int j = 1; j <= i; j++) {
                cout << i << " " << '\n';
            }

        }
        cout << "____________________________________________________________________" << endl;
    }


    //2
    {
        float a, b;
        cout << "Задание 2" << endl;
        cout << "Введите два положительных числа A и B (A > B): ";
        cin >> a >> b;

        while (a >= b) {
            a = a - b;
        }

        cout << "Длина незанятой части отрезка A равна " << a << endl;
        cout << "____________________________________________________________________" << endl;
    }


    //3
    {
        int n, k(0), s(0);
        cout << "Задание 3" << endl;
        cout << "Введите целое число N (> 1): ";
        cin >> n;

        while (s < n) {
            k++;
            s = s + k;
        }

        cout << "Число К равно " << k << endl;
        cout << "____________________________________________________________________" << endl;
    }


    //4
    {
        float p;
        cout << "Задание 4" << endl;
        cout << "Введите количество процентов P: ";
        cin >> p;
        int k = 1;
        float s = 1000;
        while (s <= 1100) {
            ++k;
            s += s * p / 100;
        }
        cout << "Количество месяцев равно " << k << endl << "Итоговый размер вклада равен " << s << '\n';
        cout << "____________________________________________________________________" << endl;
    }


    //5
    {
        int a, b;
        cout << "Задание 5" << endl;
        cout << "Введите два целых положительных числа A и B: ";
        cin >> a >> b;

        while (a != b) {
            if (a > b) {
                a = a - b;
            }
            else {
                b = b - a;
            }
        }

        cout << "НОД равен " << a << endl;
        cout << "____________________________________________________________________" << endl;
    }

    //6
    {
        int n, n1(1), n2(1), k(2), s(0);
        cout << "Задание 6" << endl;
        cout << "Введите целое число N (> 1), являющееся числом Фибоначчи: ";
        cin >> n;

        while (s != n) {
            k++;
            s = n1 + n2;
            n1 = n2;
            n2 = s;
        }

        cout << "Порядковый номер числа Фибоначчи N равен " << k << endl;

    }

    return 0;
}
