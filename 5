cout << "y1: ";
cin >> y1;
cout << "y2: ";
cin >> y2;
cout << "Расстояние равно: " << fixed << setprecision(2) << sqrt(pow(x1 - x2, 2) + pow(y1 - y2, 2)) << endl;
