	cin >> x >> y;

	if (((x == 'N') && (y == 0)) || ((x == 'W') && (y == -1)) || ((x == 'E') && (y == 1))) {
		cout << "Север" << '\n';
	}
	if (((x == 'S') && (y == 0)) || ((x == 'E') && (y == -1)) || ((x == 'W') && (y == 1))) {
		cout << "Юг" << '\n';
	}
	if (((x == 'E') && (y == 0)) || ((x == 'N') && (y == -1)) || ((x == 'S') && (y == 1))) { 
		cout << "Восток" << '\n';
	}
	if (((x == 'W') && (y == 0)) || ((x == 'S') && (y == -1)) || ((x == 'N') && (y == 1))) {
		cout << "Север" << '\n';
	}
