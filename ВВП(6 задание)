
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
cout << "Задание 1" << '\n';   
float A,B,B1;
cout << "A: ";
cin >> A;
cout << "B: ";
cin >> B;
B1 = A;
A = B;
B = B1;
cout << "Изменение порядка чисел" << '\n';
cout << "A': " << A << '\n' << "B': " << B << '\n';

cout << "Задание 2" << '\n';   
float A, B, C, X;
cout << "A: ";
cin >> A;
cout << "B: ";
cin >> B;
cout << "C: ";
cin >> C;
X = A;
A = B;
B = C;
C = X;

cout << "A': " << A << '\n' << "B': " << B << '\n' << "C': " << C;

cout << "Задание 3" << '\n';   
float a, b, c, x, y;
cout << "Введите 3 числа " << endl;
cout << "A = "; cin >> a;
cout << "B = "; cin >> b;
cout << "C = "; cin >> c;
cout << "\n";
x = a; y = b;
a = c; b = x; c = y;
cout << "A' = " << a << endl;
cout << "B' = " << b << endl;
cout << "C' = " << c << endl;

cout << "Задание 4" << '\n';   
double y;
double x;
cout << "Введите значение x: ";
cin >> x;
y = (3 * pow(x,6)) - (6 * pow(x,2)) - 7;
cout << "значение функции равно: " << y;

cout << "Задание 5" << '\n';   
double y;
double x;
cout << "Введите значение x: ";
cin >> x;
y = 4 * ((x-3) * pow(x-3,6)) - 7 * ((x-3) * pow(x-3,3)) + 2;
cout << "значение функции равно: " << y;

cout << "Задание 6" << '\n';   
double a, x;
cout << "Введите число A: ";
cin >> a;
x = a * a;   
x = x * x;    
x = x * x;    
cout << "А в 8 степени: " << x << endl;

cout << "Задание 7" << '\n';   
double x, y, a;
cout << "Введите чилсо А: ";
cin >> a;
x = a * a; 
y = x * a; 
x = x * y; 
y = x; 
x = y * y; 
x = x * y; 
cout << "А в 15 степени равно: " << x << endl;

