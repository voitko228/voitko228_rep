
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
cout << "Задание 1" << '\n';   
float A,B,B1;
cout << "A: ";
cin >> A;
cout << "B: ";
cin >> B;
B1 = A;
A = B;
B = B1;
cout << "Изменение порядка чисел" << '\n';
cout << "A': " << A << '\n' << "B': " << B << '\n';

cout << "Задание 2" << '\n';   
float A, B, C, X;
cout << "A: ";
cin >> A;
cout << "B: ";
cin >> B;
cout << "C: ";
cin >> C;
X = A;
A = B;
B = C;
C = X;

cout << "A': " << A << '\n' << "B': " << B << '\n' << "C': " << C;

cout << "Задание 3" << '\n';   
float A, B, C, X, Y;
cout << "A: ";
cin >> A;
cout << "B: ";
cin >> B;
cout << "C: ";
cin >> C;
X = A;
Y = B;
A = C;
B = X;
C = Y;

cout << "A': " << A << '\n' << "B': " << B << '\n' << "C': " << C;

cout << "Задание 4" << '\n';   
double y;
double x;
cout << "Введите значение x: ";
cin >> x;
y = (3 * pow(x,6)) - (6 * pow(x,2)) - 7;
cout << "значение функции равно: " << y;


