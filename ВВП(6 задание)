
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
cout << "Задание 1" << '\n';   
float A,B,B1;
cout << "A: ";
cin >> A;
cout << "B: ";
cin >> B;
B1 = A;
A = B;
B = B1;
cout << "Изменение порядка чисел" << '\n';
cout << "A': " << A << '\n' << "B': " << B << '\n';

cout << "Задание 2" << '\n';   
float A, B, C, X;
cout << "A: ";
cin >> A;
cout << "B: ";
cin >> B;
cout << "C: ";
cin >> C;
X = A;
A = B;
B = C;
C = X;

cout << "A': " << A << '\n' << "B': " << B << '\n' << "C': " << C;


